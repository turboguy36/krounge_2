<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <solid android:color="#00000000" />

            <stroke
                android:width="4dp"
                android:color="@color/file_search_view_background" />

        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#00000000" />

            <stroke
                android:dashGap="0dp"
                android:dashWidth="1dp"
                android:width="4dp"
                android:color="@color/file_search_view_background" />

            <padding
                android:bottom="7dp"
                android:left="7dp"
                android:right="7dp"
                android:top="7dp" />

            <corners android:radius="10dp" />
        </shape>
    </item>

</layer-list>